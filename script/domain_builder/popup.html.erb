<div id="popup_blankout"></div>
<div id="popup_white">
  <iframe src="<%= @popup_url %>"></iframe>
  <p>
    <a href="#" onclick="document.getElementById('popup_white').style.display = 'none';document.getElementById('popup_blankout').style.display = 'none';return false;">
      Close Window
    </a>
  </p>
</div>
<script type="text/javascript">function popup_blankout(element){document.getElementById('popup_blankout').style.height = (window.innerHeight) ? window.innerHeight : document.documentElement.clientHeight;}window.onload = popup_blankout;</script>
<style type="text/css">#popup_blankout{position:fixed;width:100%;height:100%;left:0px;top:0px;z-index:9999;background-color:#000000;-moz-opacity:0.8;opacity:.80;filter:alpha(opacity=80);}#popup_white{width:700px;background-color:#ffffff;height:500px;left:50%;top:50%;margin:-250px 0 0 -350px;position:fixed;z-index:10000;}#popup_white iframe{width:100%;height:90%;}</style>